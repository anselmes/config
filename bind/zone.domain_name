$TTL    604800
@       IN      SOA     ns.${domain_name}. admin.${domain_name}. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.${domain_name}.
ns      IN      A       ${server_ip}
ns      IN      AAAA    ${server_ip_v6}
